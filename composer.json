{
  "name": "butschster/laravel-git-hooks",
  "description": "Tools for managing git hooks for Laravel application",
  "keywords": [
    "laravel6",
    "laravel5",
    "lumen",
    "git",
    "git-hooks"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Buchnev",
      "email": "butschster@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/container": "^5.6|^6.0",
    "illuminate/config": "^5.6|^6.0",
    "illuminate/support": "^5.6|^6.0",
    "illuminate/contracts": "^5.6|^6.0",
    "illuminate/console": "^5.6|^6.0",
    "illuminate/pipeline": "^5.6|^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Butschster\\GitHooks\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Butschster\\GitHooks\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Butschster\\GitHooks\\Providers\\GitHooksServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
